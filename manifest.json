{
    "manifest_version": 3,
    "name": "Quick To-Do List",
    "version": "1.0",
    "description": "A simple pinned to-do list for quick tasks.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["storage"],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  